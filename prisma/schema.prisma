generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_URL")
}

model fcpy_companies {
  name                   String?                  @db.VarChar(50)
  created_at             DateTime?                @db.Timestamp(6)
  updated_at             DateTime?                @db.Timestamp(6)
  id                     Int                      @id @default(autoincrement())

}

model fcpy_employees {
 // INSERT_INTO_public_fcpy_employees__alternative_code String?   @map("INSERT INTO public.fcpy_employees (alternative_code") @db.VarChar(64)
  names                                               String?   @db.VarChar(50)
  last_names                                          String?   @db.VarChar(50)
  type                                                String?   @db.VarChar(50)
  status                                              String?   @db.VarChar(50)
  model_data                                          String?   @db.VarChar(50)
  created_at                                          DateTime? @db.Timestamp(6)
  updated_at                                          DateTime? @db.Timestamp(6)
  fcpy_payroll_id                                     Int?
  fcpy_job_position_id                                Int?
  id                                                  Int       @id @default(autoincrement())
  alternative_code                                    String?   @db.VarChar
}

model fcpy_job_positions {
  //INSERT_INTO_public_fcpy_job_positions__fcpy_work_center_id String?   @map("INSERT INTO public.fcpy_job_positions (fcpy_work_center_id") @db.VarChar(50)
  name                                                       String?   @db.VarChar(50)
  created_at                                                 DateTime? @db.Timestamp(6)
  updated_at                                                 DateTime? @db.Timestamp(6)
  fcpy_company_id                                            Int?
  id                                                         Int       @id @default(autoincrement())
  fcpy_work_center_id                                        Int?
}

model fcpy_payrolls {
  //INSERT_INTO_public_fcpy_payrolls__fcpy_work_center_id String?   @map("INSERT INTO public.fcpy_payrolls (fcpy_work_center_id") @db.VarChar(64)
  code                                                  String?   @db.VarChar(50)
  description                                           String?   @db.VarChar(64)
  frequency                                             String?   @db.VarChar(50)
  updated_at                                            DateTime? @db.Timestamp(6)
  fcpy_work_center_id                                   Int?
  id                                                    Int       @id @default(autoincrement())
  created_at                                            DateTime? @db.Timestamp(6)
}

model fcpy_work_centers {
 // INSERT_INTO_public_fcpy_work_centers__fcpy_company_id String?   @map("INSERT INTO public.fcpy_work_centers (fcpy_company_id") @db.VarChar(64)
  name                                                  String?   @db.VarChar(50)
  created_at                                            DateTime? @db.Timestamp(6)
  fcpy_company_id                                       Int?
  updated_at                                            DateTime? @db.Timestamp(6)
  id                                                    Int       @id @default(autoincrement())
}

model agr_places {
  fcpy_company_id Int?
  name            String? @db.VarChar(50)
  imagen          String? @db.VarChar(50)
  lng             String? @db.VarChar(50)
  lat             String? @db.VarChar(50)
  id              Int     @id @default(autoincrement())
}

model fcpy_activities {
  name             String?   @db.VarChar(50)
  abbreviation     String?   @db.VarChar(50)
  subclasification String?   @db.VarChar(50)
  measurement_unit String?   @db.VarChar(50)
  created_at       DateTime? @db.Timestamp(6)
  updated_at       DateTime? @db.Timestamp(6)
  aplica_extra     Int?
  fcpy_group_id    Int?
  id               Int     @id  @default(autoincrement())


}

model fcpy_groups {
  id               BigInt    @id @default(autoincrement())
  fcpy_category_id BigInt
  name             String    @db.VarChar(255)
  item             String    @db.VarChar(255)
  created_at       DateTime? @db.Timestamp(0)
  updated_at       DateTime? @db.Timestamp(0)
}

model agr_executions {
  id                  BigInt    @id @default(autoincrement())
  fcpy_activity_id    BigInt
  fcpy_cost_center_id BigInt
  fcpy_employee_id    BigInt
  supervisor          BigInt
  fcpy_work_center_id BigInt
  production          Decimal   @db.Decimal(10, 4)
  price               Decimal   @db.Decimal(8, 4)
  date                DateTime  @db.Timestamp(0)
  created_at          DateTime? @db.Timestamp(0)
  created_by          BigInt?
  updated_at          DateTime? @db.Timestamp(0)
  updated_by          BigInt?
  currency            String?   @db.VarChar(5)
  status              Int?
  source              BigInt?
  commentary          String?   @db.VarChar(500)
}

model agr_executions_log {
  id                  BigInt   @id @default(autoincrement())
  id_agr_execution    BigInt
  fcpy_activity_id    BigInt
  fcpy_cost_center_id BigInt
  fcpy_employee_id    BigInt
  supervisor          BigInt
  fcpy_work_center_id BigInt
  production          Decimal   @db.Decimal(10, 4)
  price               Decimal   @db.Decimal(8, 4)
  date                DateTime  @db.Timestamp(0)
  created_at          DateTime? @db.Timestamp(0)
  created_by          BigInt?
  updated_at          DateTime? @db.Timestamp(0)
  updated_by          BigInt?
  currency            String?   @db.VarChar(5)
  status              Int?
  source              BigInt?
  commentary          String?   @db.VarChar(500)


}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model agr_plannings {
  id                  Int     @id  @default(autoincrement())
  fcpy_activity_id    BigInt
  fcpy_cost_center_id BigInt
  fcpy_employee_id    BigInt
  supervisor          BigInt
  fcpy_work_center_id BigInt
  date                DateTime  @db.Timestamp(0)
  created_at          DateTime? @db.Timestamp(0)
  created_by          BigInt?
  updated_at          DateTime? @db.Timestamp(0)
  updated_by          BigInt?
  status              Int?
  source              BigInt?

  
}

model fcpy_cost_centers {
  id                      Int            @id  @default(autoincrement())
  fcpy_company_id         BigInt
  description             String
  abbreviation            String         @db.VarChar(255)
  accounting_nomenclature String         @db.VarChar(255)
  category                String         @db.VarChar(255)
  created_at              DateTime?      @db.Timestamp(0)
  updated_at              DateTime?      @db.Timestamp(0)
  apply_order             Boolean?
  status                  String?        @db.VarChar(5)
 
}

model fcpy_production_orders {
  id              BigInt         @id @default(autoincrement())
  fcpy_company_id BigInt
  series_name     String         @db.VarChar(255)
  doc_num         BigInt
  created_at      DateTime?      @db.Timestamp(0)
  updated_at      DateTime?      @db.Timestamp(0)
  status          String?        @db.VarChar
 
}

model sec_users {
  login           String  @id @db.VarChar(255)
  pswd            String  @db.VarChar(255)
  name            String? @db.VarChar(64)
  email           String? @db.VarChar(64)
  active          String? @db.VarChar(1)
  activation_code String? @db.VarChar(32)
  priv_admin      String? @db.VarChar(1)
  mfa             String? @db.VarChar(255)
  picture         Bytes?
  employee_code   String? @db.VarChar(255)
  id              Int?    @default(autoincrement())
}
